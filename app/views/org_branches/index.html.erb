<p id="notice"><%= notice %></p>

<h1>Org Branches</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Country</th>
      <th>State</th>
      <th>Addres</th>
      <th>Organizations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @org_branches.each do |org_branch| %>
      <tr>
        <td><%= org_branch.name %></td>
        <td><%= org_branch.country %></td>
        <td><%= org_branch.state %></td>
        <td><%= org_branch.addres %></td>
        <td><%= org_branch.organizations_id %></td>
        <td><%= link_to 'Show', org_branch %></td>
        <td><%= link_to 'Edit', edit_org_branch_path(org_branch) %></td>
        <td><%= link_to 'Destroy', org_branch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org Branch', new_org_branch_path %>
